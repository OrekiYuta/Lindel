{
  "manifest_version": 3,
  "name": "Lindel",
  "version": "0.0.1",
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_icon": {
      "32": "assets/img/202-wobbuffet.png",
      "64": "assets/img/202-wobbuffet@2x.png",
      "96": "assets/img/202-wobbuffet@3x.png"
    }
  },
  "options_page": "lindel.html",
  "background": {
    "service_worker": "assets/js/background.js"
  },
  "icons": {
    "32": "assets/img/202-wobbuffet.png",
    "64": "assets/img/202-wobbuffet@2x.png",
    "96": "assets/img/202-wobbuffet@3x.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mops1-ct3.kb.southeastasia.azure.elastic-cloud.com:9243/*"
      ],
      "js": [
        "assets/js/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "pages/panel.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
